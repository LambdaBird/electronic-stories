{
  "name": "electronic-stories",
  "version": "0.1.1",
  "description": "Run your react-stories in electron in Electron environment",
  "repository": {
    "type": "git",
    "url": "https://github.com/LambdaBird/electronic-stories.git"
  },
  "scripts": {
    "build-main": "webpack --env.NODE_ENV=production --env.DEBUG_PROD=true --config webpack.main.prod.js --colors --progress --profile",
    "build-renderer": "webpack --env.NODE_ENV=production --config webpack.prod.js --colors --progress --profile",
    "dev": "webpack-dev-server --port 9001 --env.NODE_ENV=development --config webpack.dev.js --colors",
    "start-main-dev": "electron -r '@babel/register' app/main",
    "start": "electron lib/main.prod.js"
  },
  "bin": {
    "electronic-stories": "./cli.js"
  },
  "dependencies": {
    "@babel/polyfill": "7.0.0",
    "@babel/runtime": "^7.1.2",
    "ramda": "^0.25.0",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-hot-loader": "4.3.6",
    "styled-components": "^4.1.1",
    "electron": "3.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/plugin-external-helpers": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-proposal-export-default-from": "7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "babel-eslint": "9.0.0",
    "babel-loader": "8.0.2",
    "babel-plugin-import": "1.8.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "1.7.1",
    "concurrently": "4.0.1",
    "css-loader": "1.0.0",
    "electron-builder": "^20.28.4",
    "electron-debug": "2.0.0",
    "electron-devtools-installer": "2.2.4",
    "electron-updater": "3.0.3",
    "eslint": "5.5.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.11.1",
    "file-loader": "2.0.0",
    "flow-bin": "^0.86.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "less": "3.8.1",
    "less-loader": "4.1.0",
    "style-loader": "0.23.0",
    "uglifyjs-webpack-plugin": "1.3.0",
    "url-loader": "^1.1.2",
    "webpack": "4.17.2",
    "webpack-bundle-analyzer": "2.13.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "3.1.7",
    "webpack-merge": "4.1.4",
    "write-file-webpack-plugin": "^4.4.1"
  },
  "author": "Dmytro Shaforostov",
  "license": "MIT"
}
